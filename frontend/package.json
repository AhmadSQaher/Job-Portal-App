{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "set ANALYZE=true && vite build",
    "build:prod": "set NODE_ENV=production && vite build",
    "preview": "vite preview",
    "preview:prod": "vite preview --port 4173 --host",
    "serve": "vite preview --port 4173",
    "lighthouse": "lighthouse http://localhost:4173 --output=html --output-path=lighthouse-report.html --chrome-flags=\"--headless --no-sandbox\"",
    "lighthouse:prod": "npm run build:prod && npm run preview:prod",
    "perf:audit": "npm run build:prod && npm run lighthouse:prod",
    "size:analyze": "npm run build:analyze"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@splinetool/react-spline": "^4.1.0",
    "@splinetool/runtime": "^1.10.44",
    "framer-motion": "^12.23.9",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@vitejs/plugin-legacy": "^7.1.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.19",
    "compression": "^1.8.1",
    "lighthouse": "^12.8.0",
    "postcss": "^8.4.35",
    "postcss-custom-properties": "^14.0.6",
    "puppeteer": "^24.15.0",
    "rollup": "^4.46.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "vite": "^7.0.6",
    "vite-bundle-analyzer": "^1.1.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
